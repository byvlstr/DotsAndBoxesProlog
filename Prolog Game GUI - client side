<script src="jquery-1.7.2.min.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/buzz/1.1.10/buzz.min.js"></script>
<script type="text/javascript">
var currentPlayer = "user";
var currentBoard = [];
var grilleTaille = 3;
var mode = 1;

var niveau = 4;
var niveau2 = 4;


var winSound = new buzz.sound( "win", { formats: ["mp3" ]});
var loseSound = new buzz.sound( "lose", { formats: ["mp3" ]});
var clickSound = new buzz.sound( "click", { formats: ["mp3" ]});

$(document).on("click", ".type_jeu", function(e) {
	grilleTaille = $(".intro .choix_taille_grille").val();
	niveau = $(".intro .choix_niveau_difficulte").val();
	niveau2 = $(".intro .choix_niveau_difficulte2").val();
	
	$(".intro").fadeOut(function() { 
		$(".jeu").fadeIn();
		genererGrillePoints(grilleTaille);
		genererGrille(grilleTaille); 
	});
	
	mode = $(this).attr("data-mode");
	if(mode == 1) {
		$(".score.left h2").text("Vous");
		$(".score.right h2").text("Ordinateur");	
	}
	else if(mode == 2) {
		$(".score.left h2").text("Joueur 1");
		$(".score.right h2").text("Joueur 2");	
	}
	else if(mode == 3) {
		$(".score.left h2").text("Ordinateur 1");
		$(".score.right h2").text("Ordinateur 2");	
	}
	$(".score").removeClass("active");
	$(".score.user").addClass("active");
	
	popupTextes();
	
	clickSound.play();
});

$(document).on("click", ".border:not(.on)", function() {
	
	action = $(this).attr("class").split(" ")[1];
	index = $(this).parent().attr("id");
	
	move(index, action);
	clickSound.play();
});


$(document).on("click", ".go_menu", function() {
	
	$(".jeu").fadeOut(function() { 
		$(".intro").fadeIn();
	});
	rejouer();
});

$(document).on("click", ".rejouer", function() {
	rejouer();
});

function rejouer() {
	$(".border.on").removeClass("on user computer");
	$("#grille td").removeClass("complet complet_user complet_computer");
	currentPlayer = "user";
	genererGrillePoints();
	
	$("#score_user").text(0);
	$("#score_computer").text(0);
	
	$("#mask").fadeOut();
	$(".popup").fadeOut();
}

function genererGrillePoints() {
	
	for(i=0; i < grilleTaille*grilleTaille; i++)
		currentBoard[i] = 0;
}

function genererGrille(nb) {
	$("#grille").empty();
	
	index=0;
	
	$("#grille").append("<tr></tr>");
	
	$("#grille tr").last().append('  \
		<td id="'+(index++)+'"> \
			<div class="point" style="left: -10px;top: -10px;"></div> \
			<div class="border left"></div> \
			<div class="border top"></div> \
			<div class="border right"></div> \
			<div class="border bottom"></div> \
		 </td>');
	 
	for(j=0; j < nb-2; j++) {
		$("#grille tr").last().append('  \
				<td id="'+(index++)+'"> \
					<div class="point" style="left: -10px;top: -10px;"></div> \
					<div class="border left"></div> \
					<div class="border top"></div> \
					<div class="border right"></div> \
					<div class="border bottom"></div> \
				</td>');
	}
	$("#grille tr").last().append('  \
		<td id="'+(index++)+'"> \
			<div class="point" style="left: -9px;top: -10px;"></div> \
			<div class="point" style="right: -9px;top: -10px;"></div> \
			<div class="border left"></div> \
			<div class="border top"></div> \
			<div class="border right"></div> \
			<div class="border bottom"></div> \
	     </td>');
		     
	for(i=0; i < nb-2; i++) {
		$("#grille").append("<tr></tr>");
		
		$("#grille tr").last().append('  \
			<td id="'+(index++)+'"> \
            	<div class="point" style="left: -9px;top: -9px;"></div> \
				<div class="border top"></div> \
				<div class="border left"></div> \
				<div class="border right"></div> \
				<div class="border bottom"></div> \
			 </td>');
		 
		for(j=0; j < nb-2; j++) {
			$("#grille tr").last().append('  \
					<td id="'+(index++)+'"> \
			            	<div class="point" style="left: -9px;top: -9px;"></div> \
						<div class="border top"></div> \
						<div class="border left"></div> \
						<div class="border right"></div> \
						<div class="border bottom"></div> \
					</td>');
		}
		$("#grille tr").last().append('  \
			<td id="'+(index++)+'"> \
				<div class="point" style="left: -9px;top: -9px;"></div> \
				<div class="point" style="right: -9px;top: -9px;"></div> \
				<div class="border top"></div> \
				<div class="border left"></div> \
				<div class="border right"></div> \
				<div class="border bottom"></div> \
		     </td>');
	}
	$("#grille").append("<tr></tr>");
	
	$("#grille tr").last().append('  \
		<td id="'+(index++)+'"> \
			<div class="point" style="left: -9px;top: -9px;"></div> \
			<div class="point" style="left: -9px;bottom: -9px;"></div> \
			<div class="border top"></div> \
			<div class="border left"></div> \
			<div class="border right"></div> \
			<div class="border bottom"></div> \
		 </td>');
	 
	for(j=0; j < nb-2; j++) {
		$("#grille tr").last().append('  \
				<td id="'+(index++)+'"> \
					<div class="point" style="left: -9px;top: -9px;"></div> \
					<div class="point" style="left: -9px;bottom: -9px;"></div> \
					<div class="border top"></div> \
					<div class="border left"></div> \
					<div class="border right"></div> \
					<div class="border bottom"></div> \
				</td>');
	}
	$("#grille tr").last().append('  \
		<td id="'+(index++)+'"> \
			<div class="point" style="left: -9px;top: -9px;"></div> \
			<div class="point" style="right: -9px;top: -9px;"></div> \
			<div class="point" style="left: -9px;bottom: -9px;"></div> \
			<div class="point" style="right: -9px;bottom: -9px;"></div> \
			<div class="border top"></div> \
			<div class="border left"></div> \
			<div class="border right"></div> \
			<div class="border bottom"></div> \
	     </td>');
	     
	$("#grille td").css("height", $("#grille td").width() + "px");

}

function move(index, action) {
	board = "";
	
	for(i=0; i < grilleTaille*grilleTaille -1; i++)
		board +=  currentBoard[i]+".";
	
	board += currentBoard[grilleTaille*grilleTaille -1];	

	//on active le 2e niveau d'IA lorsque 2 ordis se confrontent
	curNiveau = niveau;
	if(mode == 3 && currentPlayer == "user")
		curNiveau = niveau2;

	jQuery.ajax({
		url: 'http://localhost:5000/game',
		data : {
			"turnIndex" : index,
			"turnAction" : action,
			"board" : board,
			"scoreUser1" : $("#score_user").text(),
			"scoreUser2" : $("#score_computer").text(),
			"user" : currentPlayer,
			"dim":grilleTaille,
			"mode":mode,
			"niveau":curNiveau
		},
		type:"GET",
		dataType: 'JSON',
		success: function(data) {
			currentBoard = data.board;
			
			for(i = 0; i < grilleTaille*grilleTaille; i++) {
				bin = parseInt(currentBoard[i], 10).toString(2);
				bin = pad(bin, 4);
				bin = bin.split("");
				
				if(bin[0] == 1 && !$("td#"+i).find(".bottom").hasClass("on"))
					$("td#"+i).find(".bottom").addClass("on "+ currentPlayer);
					
				if(bin[1] == 1 && !$("td#"+i).find(".left").hasClass("on"))
					$("td#"+i).find(".left").addClass("on "+ currentPlayer);
					
				if(bin[2] == 1 && !$("td#"+i).find(".top").hasClass("on"))
					$("td#"+i).find(".top").addClass("on "+ currentPlayer);
					
				if(bin[3] == 1 && !$("td#"+i).find(".right").hasClass("on"))
					$("td#"+i).find(".right").addClass("on "+ currentPlayer);
					
				if(!$("td#"+i).hasClass("complet") && bin[0] ==  1 && bin[1] ==  1 && bin[2] ==  1 && bin[3] ==  1) {
					$("td#"+i).addClass("complet complet_"+currentPlayer);
						
					if(currentPlayer == "user")
						winSound.play();
					else
						loseSound.play();
				}
			}
			
			$("#score_user").text(data.score_user1);
			$("#score_computer").text(data.score_user2);
			
			if(data.game_over) {
				if(data.gagnant == "aucun")
					$("#user_draw").fadeIn();
				else
					$("#"+data.gagnant+"_win").fadeIn();
					
				$("#mask").fadeIn();
				return;
			}
			
			currentPlayer = data.next_player;
			
			$(".score").removeClass("active");
			$(".score."+currentPlayer).addClass("active");
			
			if( mode == 1) {
				if(currentPlayer == "computer")
					setTimeout(function() {
						move(0,0);
					}, 300);
			}
			else if( mode == 3) {
				setTimeout(function() {
					move(0,0);
				}, 300);
			}
		}
	});
}

function popupTextes() {
	if(mode == 1) {
		$("#user_win h3").text("Victoire !");
		$("#user_win p").text("Bravo ! Vous avez gagné !");
		$("#computer_win h3").text("C'est la défaite...");
		$("#computer_win p").text("L'ordinateur a été plus fort que vous !");
		$("#user_draw p").text("L'ordinateur ne vous a pas battu, c'est déjà ça !");
	}
	else if(mode == 2) {
		$("#user_win h3").text("Joueur 1 gagne !");
		$("#user_win p").text("Bravo ! Vous méritez un cookie !");
		$("#computer_win h3").text("Joueur 2 gagne !");
		$("#computer_win p").text("Respect, respect....");
		$("#user_draw p").text("Allez, une revanche ? Hein, hein ?");
	}
	else if(mode == 3) {
		$("#user_win h3").text("Ordinateur 1 gagne !");
		$("#user_win p").text("Ce n'est pas le number 1 pour rien !");
		$("#computer_win h3").text("Ordinateur 2 gagne !");
		$("#computer_win p").text("Parfait, j'avais parié 20€ sur lui !");
		$("#user_draw p").text("Tout ça pour ça...");
	}
}

function pad (str, max) {
  str = str.toString();
  return str.length < max ? pad("0" + str, max) : str;
}
</script>

